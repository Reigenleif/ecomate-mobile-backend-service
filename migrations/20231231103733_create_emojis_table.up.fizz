create_table("Emojis") {
    t.Column("id", "uuid", { "default_raw" : "uuid_generate_v1()"})
    t.Column("name", "varchar(255)")
    t.Column("image", "varchar(255)")
    t.Column("created_at", "timestamp", { "default_raw" : "now()"})
    t.Column("updated_at", "timestamp", { "default_raw" : "now()"})
    t.PrimaryKey("id")
}